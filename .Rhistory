source('~/GitHub/protozoa/genetic_div_human_p.R')
rm(list = ls())
phi_data<-read.csv('results.csv',header=T)
library(vegan)
fit <- manova(cbind(Species,Gene,Phi,Theta) ~ Zoonotic, data = phi_data)
summary(fit, test="Pillai")
